    "export": "yarn ts-next export",
    "build": "yarn build-app-image-meta && yarn ts-next build",
    "start": "yarn ts-next start",
    "lint": "eslint . --format=codeframe",
    "type-check": "tsc --noEmit",
    "migrate": "yarn local-ts-node ./lib/server/scripts/migration/migrate-content.ts",
    "build-app-image-meta": "NODE_OPTIONS='--max-old-space-size=16384' yarn local-ts-node ./lib/server/scripts/build-app-image-meta.ts"
  },
